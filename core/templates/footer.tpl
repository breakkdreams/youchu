                    if($('.indexw_friend_link ul').length>1){
                        friend_con = setInterval(function(){
                            $('.indexw_friend_link ul:last').hide().insertBefore($(".indexw_friend_link ul:first")).slideDown(1000);
                        }, 3000);
                    }
                    $('.indexw_friend_link ul').mouseover(function(){
                        clearInterval(friend_con);
                    }).mouseout(function(){
                        if($('.indexw_friend_link ul').length>1){
                            friend_con = setInterval(function(){
                                $('.indexw_friend_link ul:last').hide().insertBefore($(".indexw_friend_link ul:first")).slideDown(1000);
                            }, 3000);
                        }
                    });
                    /***top菜单效果***/

                    var comment_con,comment_num,com_index=0;
                    comment_num = $('.box_dl_div .box_dl').length;
                    if(comment_num>1){
                        comment_con = setInterval(function(){
                            if(com_index==(comment_num-1)){
                                $(".box_dl_div .box_dl:eq("+com_index+")").stop(true,true).hide();
                                $(".box_dl_div .box_dl:eq(0)").stop(true,true).slideDown(1000);
                                com_index = 0;
