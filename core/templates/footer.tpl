<script type="text/javascript">
    //登陆验证方法
  function check_login_email(username){
     if (username == ''){
        $(".text-password").after('<li class="error_msg" style="color:red;">您输入的账号为空，请重新输入。</li>');
        return false;
     }
  }

  function check_login_password(login_password){
       if (login_password == ''){
          $(".text-password").after('<li class="error_msg" style="color:red;">您输入的密码为空，请重新输入。</li>');
          return false;
       }
  }

  $(function(){
    //登陆
    $("#member_login").click(function(){
      var login_email = $.trim($("#login_email").val());
      var login_password = $("#login_password").val();
      if($(".error_msg")){
            $(".error_msg").remove();
          }
      if ( check_login_email(login_email) == false || check_login_password(login_password) == false){
          return false;
      }
      $.post("/web/check_login",{username:login_email,passwd:login_password},function(data){
          var result = eval('('+data+')');
          if(result['result']=='succ'){
            $("#useway").val(result['useway']);
            $("#login_form").submit();
          }else{
            $(".text-password").after('<li class="error_msg" style="color:red;">您输入的'+result['msg']+'，请重新输入。</li>');
          }
      });
    });

    $("#login-box-close").click(function(){
      $("#pageOverlay").hide();
      $(".loginmini-box").hide();
    });

    $("#regist").click(function(){
      location.href="/index.php/web/login?hot=1";
    });

  });
</script>
